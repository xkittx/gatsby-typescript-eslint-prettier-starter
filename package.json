{
  "name": "gatsby-typescript-eslint-prettier-starter",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "develop": "gatsby develop",
    "start": "gatsby develop",
    "build": "gatsby build",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "typecheck": "tsc --noEmit",
    "lint": "yarn run eslint --ignore-path .gitignore",
    "lint:fix": "yarn run eslint --ignore-path .gitignore --fix",
    "format": "prettier --write . --ignore-path .gitignore",
    "allformat": "yarn lint:fix && yarn format"
  },
  "dependencies": {
    "gatsby": "4.15.1",
    "react": "18.1.0",
    "react-dom": "18.1.0"
  },
  "devDependencies": {
    "@types/node": "17.0.36",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.5",
    "@typescript-eslint/eslint-plugin": "5.27.0",
    "@typescript-eslint/parser": "5.27.0",
    "eslint": "8.16.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-react": "7.30.0",
    "prettier": "2.6.2",
    "typescript": "4.7.2"
  }
}
